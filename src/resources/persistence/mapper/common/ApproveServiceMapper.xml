<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.skshieldus.esecurity.repository.common.ApproveServiceRepository">

	<select id="selectInoutDelayCalling" parameterType="Integer" resultType="Map">
		SELECT
		    FILE_ENC_IMG
		FROM SCINOUTADM.INOUT_DELAY_CALLING_HISTORY
		WHERE HIST_SEQ_NO = #{histSeqNo}
	</select>

</mapper>
